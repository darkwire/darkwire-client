{
  "name": "darkwire-client",
  "version": "2.0.0-beta.7",
  "main": "index.js",
  "contributors": [
    {
      "name": "Daniel Seripap"
    },
    {
      "name": "Alan Friedman"
    }
  ],
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server",
    "dev:mobile": "NODE_ENV=development webpack-dev-server --host 0.0.0.0",
    "build": "rm -rf dist && NODE_ENV=development webpack",
    "build:staging": "rm -rf dist && NODE_ENV=staging webpack",
    "build:production": "rm -rf dist && NODE_ENV=production webpack",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "test": "BABEL_DISABLE_CACHE=1 NODE_ENV=test jest --u",
    "test:dev": "jest --watch",
    "test:coverage": "BABEL_DISABLE_CACHE=1 NODE_ENV=test jest --coverage",
    "release": "./release.sh"
  },
  "dependencies": {
    "autosize": "^4.0.0",
    "bootstrap": "^4.0.0-beta.2",
    "classnames": "^2.2.5",
    "clipboard": "^1.7.1",
    "feather-icons": "^3.2.2",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.19.3",
    "popper.js": "^1.12.5",
    "query-string": "^4.3.2",
    "randomcolor": "^0.5.3",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-feather": "^1.0.7",
    "react-modal": "^2.3.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-simple-dropdown": "^3.2.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sanitize-html": "^1.14.1",
    "shortid": "^2.2.8",
    "socket.io-client": "^2.0.3",
    "tinycon": "^0.6.8",
    "webcrypto-shim": "^0.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.27.3",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.1.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^21.1.0",
    "node-sass": "^4.5.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "postcss-nested": "^2.1.2",
    "react-hot-loader": "3.1.3",
    "react-test-renderer": "v16.0.0-rc.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.4.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styles.js"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "<rootDir>/src/test/setup.js"
    ]
  }
}
